<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <session-config>
        <cookie-config>
            <http-only>true</http-only>
            <secure>false</secure>
        </cookie-config>
    </session-config>

    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>webapp.login</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>planets</servlet-name>
        <servlet-class>webapp.planets</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>planets</servlet-name>
        <url-pattern>/planets</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>planet_info</servlet-name>
        <servlet-class>webapp.planet_info</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>planet_info</servlet-name>
        <url-pattern>/planet_info</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>add_expedition</servlet-name>
        <servlet-class>webapp.add_expedition</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>add_expedition</servlet-name>
        <url-pattern>/add_expedition</url-pattern>
    </servlet-mapping>

</web-app>